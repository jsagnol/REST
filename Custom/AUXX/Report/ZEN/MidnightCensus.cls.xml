<report xmlns="http://www.intersystems.com/zen/report/definition"
 name="MidnightCensus" queryClass="Custom.AUXX.Report.ZEN.StoredProc.MidnightCensus" queryName="GetPatientList">
<parameter expression="..ReportDate" />
